<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.tomeraberbach.mano.application.Main">
    <SplitPane dividerPositions="0.5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
               AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="NEVER" maxWidth="-Infinity"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="-Infinity" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" vgrow="SOMETIMES"/>
            </rowConstraints>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <Label text="Error Log:" GridPane.columnSpan="2">
                <font>
                    <Font size="14.0"/>
                </font>
            </Label>
            <AnchorPane GridPane.columnSpan="2" GridPane.rowIndex="1">
                <GridPane.margin>
                    <Insets top="10.0"/>
                </GridPane.margin>
                <TextArea fx:id="errorFX" editable="false" prefHeight="200.0" prefWidth="200.0"
                          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </TextArea>
            </AnchorPane>
            <Button mnemonicParsing="false" onAction="#assembleOnAction" text="Assemble"
                    GridPane.halignment="LEFT" GridPane.rowIndex="2">
                <GridPane.margin>
                    <Insets top="10.0"/>
                </GridPane.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#helpOnAction" text="Help" GridPane.columnIndex="1"
                    GridPane.rowIndex="2">
                <GridPane.margin>
                    <Insets left="10.0" top="10.0"/>
                </GridPane.margin>
            </Button>
        </GridPane>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                      AnchorPane.topAnchor="0.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity"/>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity"/>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity"/>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" prefWidth="100.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints maxHeight="1.7976931348623157E308" vgrow="SOMETIMES"/>
                    <RowConstraints maxHeight="-Infinity" vgrow="SOMETIMES"/>
                </rowConstraints>
                <TabPane fx:id="tabsFX" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE"
                         GridPane.columnSpan="5">
                </TabPane>
                <Button mnemonicParsing="false" onAction="#saveOnAction" text="Save"
                        GridPane.columnIndex="2" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#saveAsOnAction" text="Save As..."
                        GridPane.columnIndex="3" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#newOnAction" text="New" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#openOnAction" text="Open..."
                        GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0"/>
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#closeOnAction" text="Close"
                        GridPane.columnIndex="4" GridPane.rowIndex="1"/>
            </GridPane>
        </AnchorPane>
    </SplitPane>
</AnchorPane>
